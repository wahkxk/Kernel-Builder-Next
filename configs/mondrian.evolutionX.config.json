              "OBJCOPY":"clang/bin/llvm-objcopy",  
              "OBJDUMP":"clang/bin/llvm-objdump",  
              "STRIP":"clang/bin/llvm-strip",  
              "LD":"clang/bin/ld.lld", 
              "LTO": "thin" 
            } 
          }, 
          "AnyKernel3": { 
            "use": true, 
            "release": true,
            "custom": {
              "repo": "https://github.com/luyanci/Ak3",
              "branch": "k60"
            }
          },
          "enableKernelSU": true, 
          "KernelSUSFS":{
            "enable": true,
            "branches": "gki-android12-5.10"
          },
          "enableLXC": false        
    }
