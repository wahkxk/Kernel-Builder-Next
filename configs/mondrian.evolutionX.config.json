[
    {
        "kernelSource": { 
            "name": "k60-evolutionX", 
            "repo": "https://github.com/Evolution-X-Devices/kernel_xiaomi_sm8475", 
            "branch": "udc", 
            "device": "mondrian", 
            "defconfig": "vendor/mondrian_defconfig" 
          }, 
          "toolchains": [ 
            {  
