[

    {
        "kernelSource": {
            "name": "PE12",
            "repo": "https://github.com/PixelExperience-Devices/kernel_xiaomi_phoenix",
            "branch": "twelve",
            "device": "phoenix",
            "defconfig": "vendor/phoenix_defconfig"
          },
          "toolchains": [
          {
            "repo": "https://gitlab.com/ThankYouMario/android_prebuilts_clang-standalone/",
            "branch": "11",
            "name": "clang",
            "binaryEnv": [
                "bin"
            ]
          },
          {
            "repo": "https://android.googlesource.com/platform/prebuilts/gas/linux-x86",
            "branch": "master",
            "name": "gas",
            "binaryEnv": [
                "bin"
            ]
          }
        ],
          "enableCcache": true,
        "params": {
          "ARCH": "arm64",
          "CC": "clang",
          "externalCommands": {
            "CLANG_TRIPLE": "aarch64-linux-gnu-",
            "CROSS_COMPILE": "aarch64-linux-gnu-",
            "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
            "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-"
          }
          },
          "AnyKernel3": {
            "use": true,
            "release": true,
            "custom": {
            "repo": "https://github.com/luyanci/AnyKernel3",
            "branch": "master"
        }
          },
          "enableKernelSU": true,
          "KernelSUSFS":{
            "enable": true,
            "branches": "kernel-4.14"
          },
          "enableLXC": false
    }
]